{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "max-len": [2, 100],
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "class-methods-use-this":  "off",
      "prefer-destructuring": "off",
      "no-undef": "off",
      "no-console": "off"
    }
}
